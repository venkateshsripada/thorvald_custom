<!-- 
    ROS navigation stack with velocity smoother and safety (reactive) controller
-->
<launch>
  <!-- Required only for turtlebot because we can't directly publish to cmd_vel in tutrlebot
  <include file="$(find thorvald_2dnav)/launch/includes/velocity_smoother.launch.xml"/>
  <include file="$(find thorvald_2dnav)/launch/includes/safety_controller.launch.xml"/>
  --> 
  
  <arg name="odom_frame_id"   default="odom"/>
  <arg name="base_frame_id"   default="base_footprint"/>
  <arg name="global_frame_id" default="map"/>
  <arg name="odom_topic" default="odom" />
  <arg name="laser_topic" default="scan" />

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find thorvald_2dnav)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
   
  </node>

</launch>